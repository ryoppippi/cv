{
  "tasks": {
    "typst": "deno run -RE --allow-run npm:typst",
    "dev": "deno task typst watch ./ryotaro_kimura.typ",
    "compile": "deno task typst compile ./ryotaro_kimura.typ --font-path ./ibm-sans",
    "update-stars": "deno run --allow-env=GITHUB_TOKEN --allow-net=api.github.com --allow-read=ryotaro_kimura.typ --allow-write=ryotaro_kimura.typ update-stars.ts"
  },
  "imports": {
    "@std/assert": "jsr:@std/assert@1.0.13"
  }
}
